@{
    var empresa = await EmpresaService.GetDadosEmpresaAsync();
    var emailEmpresa = empresa?.Email ?? Configuration["Empresa:Email"];
    var telefoneEmpresa = empresa?.Telefone ?? Configuration["Empresa:Telefone"];
    var EnderecoEmpresa = empresa?.Endereco ?? Configuration["Empresa:Endereco"];
}



<div class="container">
    <img src="~/images/faleconosco1.jpg" alt="Fale conosco" />
    <div class="jumbotron">
        <h3>Queremos ouvir o que você tem a dizer</h3>
        <p>
            Se você tiver alguma dúvida ou sugestão fale conosco
        </p>
        <p>
            <span>
                <img src="~/images/Envelope-icon24.png" />
            </span>
            <h5> @emailEmpresa </h5>
            <br />
            <span>
                <img src="~/images/phone-app-icon.png" />
            </span>
            <h5> @telefoneEmpresa</h5>
            <br />
            <span>
                <img src="~/images/Home-iconred.png" />
            </span>
            <br />
            <h5> @EnderecoEmpresa</h5>
        </p>
    </div>
</div>
